<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/util
		http://www.springframework.org/schema/util/spring-util-3.0.xsd">
	

	<bean id="listArraySetBean" class="SpringSE.collection.ListArraySet">
		<property name="list">
			<list>
				<value>B</value>
				<value>C</value>
				<value>D</value>
				<value>A</value>
			</list>
		</property>
		<property name="array">
			<array>
				<value>1</value>
				<value>2</value>
				<value>3</value>
				<value>4</value>
			</array>
		</property>
		<property name="set">
			<set>
				<value>E</value>
				<value>C</value>
				<value>B</value>
				<value>D</value>
				<value>A</value>
			</set>
		</property>
		<property name="listint">
			<list>
				<value>1</value>
				<value>3</value>
				<value>4</value>
				<value>2</value>
			</list>
		</property>
	</bean>	
	
	<bean id="test" class="SpringSE.collection.ListArraySet">
		<property name="list">
			<list>
				<value>B</value>
				<value>C</value>
				<value>D</value>
				<value>A</value>
			</list>
		</property>
		<property name="array">
			<array>
				<value>1</value>
				<value>2</value>
				<value>3</value>
				<value>4</value>
			</array>
		</property>
		<property name="set">
				<util:set set-class="java.util.TreeSet">
					<value>B</value>
					<value>C</value>
					<value>D</value>
					<value>A</value>
				</util:set>
		</property>
		<property name="listint">
			<list>
				<value>1</value>
				<value>3</value>
				<value>4</value>
				<value>2</value>
			</list>
		</property>
	</bean>
</beans>